package com.jm.java.object;

class Data11 {
    int value;
}

class Data22 {
    int value;

    Data22(int x) {
        value = x;
    }
}


public class ConstructorTest {
    public static void main(String[] args) {
    	
        Data11 d1 = new Data11();
        
        // Data22 d2 = new Data22();           // 컴파일 에러 발생 (int 인자 하나 필요함)
        Data22 d2 = new Data22(10);
    }
}